<Window x:Class="MaiksLauncher.Logger"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaiksLauncher"
        mc:Ignorable="d"
        Title="Logger" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="Log" x:FieldModifier="public" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Height="414" Width="780" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True" Text="&lt;Tap here to start the Logger&gt;"/>

    </Grid>
</Window>
